{
  "name": "frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "predev": "yarn build:tokens",
    "prebuild": "yarn build:tokens",
    "dev": "next dev",
    "build": "next build",
    "build:tokens": "node ./node_modules/@aw-web-design/theme/generate",
    "start": "next start",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:js": "npx eslint . --ext .ts,.tsx",
    "lint:css": "stylelint ./**/styled.ts",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@aw-web-design/components": "0.0.15",
    "@aw-web-design/theme": "0.0.30",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "copy-webpack-plugin": "^8.1.1",
    "csstype": "3.0.7",
    "enhanced-resolve": "5.2.0",
    "next": "^10.1.3",
    "react": "17.0.2",
    "react-dom": "16.13.1",
    "styled-components": "5.2.3",
    "styled-system": "5.1.5",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@storybook/addon-docs": "6.2.7",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.15.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "circular-dependency-plugin": "5.2.0",
    "copy-webpack-plugin": "^8.1.1",
    "css-loader": "5.2.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "7.23.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-standard": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^6.2.1",
    "html-webpack-plugin": "^5.3.1",
    "jest": "26.4.2",
    "jest-cli": "26.4.2",
    "mini-css-extract-plugin": "1.4.1",
    "prettier": "2.1.2",
    "stylelint": "^13.12.0",
    "stylelint-config-recommended": "^4.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^4.1.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-loader": "8.1.0",
    "webpack": "^5.31.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "packages/**/*.ts"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss|svg)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/setupTests.js"
    ],
    "transformIgnorePatterns": [
      "/^/node_modules/@orchard/((theme)|(theme-provider))$/"
    ],
    "resolver": "./jestAliasResolver"
  }
}
